        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/mib21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MIB 21" />

        <TextView
            android:id="@+id/mib22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MIB 22" />

        <TextView
            android:id="@+id/mib23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MIB 23" />

        <TextView
            android:id="@+id/mib24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MIB 24" />

        <TextView
            android:id="@+id/mib25"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MIB 25" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>