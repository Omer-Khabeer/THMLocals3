    <LinearLayout
        android:id="@+id/Linearlayout1"
        android:layout_width="424dp"
        android:layout_height="363dp"
        android:layout_marginTop="52dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/semester1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:background="#60B564"
            android:fontFamily="@font/baloo"
            android:gravity="center"
            android:text="Semester 1"
            android:textAlignment="center"
            android:textColor="#222020"
            android:textSize="18sp"
            android:textStyle="bold|italic" />

        <TextView
            android:id="@+id/semester2"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:background="#3CA641"
            android:fontFamily="@font/baloo"
            android:gravity="center"
            android:text="Semester 2"
            android:textAlignment="center"
            android:textColor="#1B1A1A"
            android:textSize="18sp"
            android:textStyle="bold|italic" />

        <TextView
            android:id="@+id/semester3"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:background="#03A9F4"
            android:fontFamily="@font/baloo"
            android:gravity="center"
            android:text="Semester 3"
            android:textAlignment="center"
            android:textColor="#1E1D1D"
            android:textSize="18sp"
            android:textStyle="bold|italic" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/Linearlayout2"
        android:layout_width="427dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Linearlayout1"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/semester4"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:background="#0984BC"
            android:fontFamily="@font/baloo"
            android:gravity="center"
            android:text="Semester 4 "
            android:textAlignment="center"
            android:textColor="#121212"
            android:textSize="18sp"
            android:textStyle="bold|italic" />

        <TextView
            android:id="@+id/semester5"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:background="#FFEB3B"
            android:fontFamily="@font/baloo"
            android:gravity="center"
            android:text="Semester 5"
            android:textAlignment="center"
            android:textColor="#262323"
            android:textSize="18sp"
            android:textStyle="bold|italic" />

        <TextView
            android:id="@+id/semester6"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:background="#CDDC39"
            android:fontFamily="@font/baloo"
            android:gravity="center"
            android:text="Semester 6"
            android:textAlignment="center"
            android:textColor="#1D1B1B"
            android:textSize="18sp"
            android:textStyle="bold|italic" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>


